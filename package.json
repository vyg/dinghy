    "license": "MIT",
    "author": {
        "name": "Ryan O'Hara",
        "email": "ryan@voyage.studio",
        "url": "http://voyage.studio"
    },
    "main": "dist/dinghy.css",
    "keywords": [],
    "bugs": {
        "url": "https://github.com/vyg/dinghy/issues"
    },
    "engines": {
        "node": ">=4"
    },
    "devDependencies": {
        "autoprefixer": "^9.0.1",
        "node-sass": "^7.0.0",
        "np": "^3.0.4"
    },
    "scripts": {
        "build": "node-sass --output-style expanded --source-map true src/styles.scss dist/dinghy.css  && node-sass --output-style compressed src/styles.scss dist/dinghy.min.css",
        "start": "node-sass src/styles.scss dist/dinghy.css --watch ./src",
        "release": "np --yolo"
    },
    "dependencies": {
        "normalize.css": "^8.0.0"
    }
}
